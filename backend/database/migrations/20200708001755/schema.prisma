datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Job {
  id        Int    @default(autoincrement()) @id
  type      String
  args      String
  status    String
  userId    String
  createdAt Int
  updatedAt Int
}

model Message {
  id        Int    @default(autoincrement()) @id
  userId    String
  messageId Int
}

model Result {
  id        Int    @default(autoincrement()) @id
  userId    String
  messageId String
  type      String
  data      String
}
