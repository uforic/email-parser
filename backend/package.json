{
    "name": "webpack-start",
    "version": "1.0.0",
    "author": "Gianluca Venturini <gianluca@stellarite.io>",
    "license": "MIT",
    "scripts": {
        "build": "PROJECTS=backend,cli webpack --progress --colors",
        "test": "PROJECTS=test webpack --progress --colors && node --inspect ./node_modules/jasmine/bin/jasmine.js dist/test.js",
        "build-watch": "PROJECTS=frontend,backend WATCH=true webpack --progress --colors",
        "start": "node --inspect dist/server.js",
        "clean": "rm -rf node_modules/ dist/",
        "prisma-generate": "prisma generate --schema ./database/schema.prisma",
        "prisma-migrate": "prisma migrate save --experimental --schema database/schema.prisma",
        "prisma-up": "prisma migrate up --experimental --schema database/schema.prisma",
        "prisma-reset": "rm -f database/dev.db && yarn prisma-up",
        "graphql": "graphql-codegen -c codegen.yml"
    },
    "engines": {
        "yarn": "^1.10.1",
        "node": ">=12.18.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.16.3",
        "@graphql-codegen/typescript-resolvers": "^1.16.3",
        "@prisma/cli": "^2.1.3",
        "@types/express": "^4.17.6",
        "@types/jasmine": "^3.5.10",
        "@types/node-fetch": "^2.5.7",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/webpack-env": "^1.15.2",
        "html-webpack-plugin": "^3.2.0",
        "jasmine": "^3.5.0",
        "node-fetch": "^3.1.1",
        "prettier": "2.0.5",
        "source-map-support": "^0.5.19",
        "ts-loader": "^6.2.1",
        "ts-node": "8.10.2",
        "typescript": "^3.7.4",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@prisma/client": "^2.1.3",
        "@types/async-lock": "^1.1.2",
        "@types/express-session": "^1.17.0",
        "@types/jwt-decode": "^2.2.1",
        "@types/lodash.debounce": "^4.0.6",
        "@types/lodash.memoize": "^4.0.6",
        "@types/sqlstring": "^2.2.1",
        "apollo-server": "^2.15.1",
        "apollo-server-express": "^2.15.1",
        "async-lock": "^1.2.4",
        "commander": "5.1.0",
        "dataloader": "^2.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "googleapis": "39",
        "graphql": "^15.2.0",
        "jwt-decode": "^2.2.0",
        "lodash.debounce": "^4.0.8",
        "lodash.memoize": "^4.1.2",
        "node-html-parser": "^1.2.20",
        "sqlstring": "^2.3.2",
        "sqlstring-sqlite": "^0.1.1"
    }
}
